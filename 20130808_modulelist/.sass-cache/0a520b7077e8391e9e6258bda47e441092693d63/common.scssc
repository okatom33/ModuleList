/* BGN import compass */
$experimental-support-for-mozilla: true !default;
$experimental-support-for-webkit: true !default;
$experimental-support-for-opera: true !default;
$experimental-support-for-microsoft: true !default;
@import 'compass';
$imgPath:"../images/";
$imgPathInline: "../images/";
$textColor: #fff;



body{
  font-family: Futura;
  background:#000;
  width:100%;
  height:2000px;
}

section.circle{
  width:320px;
  height:100%;
  margin:0 auto;
  padding:0;
}

h1,p{
  color:$textColor;
}


a{
  text-decoration:none;
  color:#f00;
}


%hr{
  width:120px;
  height:40px;
  background:-webkit-gradient(
    linear,left top,left bottom,
    from(#f50),
    to(#f30)
    );
  display:block;
  border:1px solid #f00;
  border-radius:3px;
  text-align:center;
  line-height:40px;
  color:#fff;
  box-shadow:0 4px 0px #820;
  margin:5px auto;
  padding:0;
  font-size:14px;
}

.normalBtn{
  @extend %hr;
  &:hover{
    box-shadow:0px 2px 5px #222 inset;
    margin-top:10px;
    border:1px solid transparent;
  }
}

.appealBtn{
  @extend %hr;
  box-shadow:none;
  background:-webkit-gradient(
    linear,left top,left bottom,
    from(#f00),
    to(#a00)
    );
  border:2px solid #ff0;
  &:hover{
    -webkit-animation:buruburu 2s  ease-in-out infinite 1s;
  }
}

@-webkit-keyframes buruburu{
  0%,99.999%,100%{
    -webkit-transform:rotate(0deg);
  }
  40%{
    -webkit-transform:rotate(0deg);
  }
  42%,44%,46%,48%,52%,54%,56%,58%{
    -webkit-transform:rotate(10deg);
  }
  43%,44%,47%,49%,53%,55%,57%,59%{
    -webkit-transform:rotate(-10deg);
  }
  60%{
    -webkit-transform:rotate(0deg);
  }
}

.lightBtn{
  @extend .appealBtn;
  border:2px solid #600;
  &:hover{
    -webkit-animation:light 2s  ease-in-out infinite 1s;
  }
}
@-webkit-keyframes light{
  0%,99.99%,100%{
    -webkit-box-shadow:0 0 3px rgba(255,255,200,0.5);
  }
  50%{
    -webkit-box-shadow:0 0 20px rgba(255,255,200,0.5),0 0 20px rgba(255,255,200,0.5),0 0 20px rgba(255,255,200,0.5),0 0 20px rgba(255,255,200,0.5);
  }
}



.p {
	height:480px;
	font-size:30px;
	background: #F0F0F0;
	background-image: -webkit-linear-gradient(#EEE,#DDD);
}

nav{
  position:relative;
  width:740px;
}
a.home{
  width:120px;
  height:120px;
  background: #f40;
  display:inline-block;
  border-radius:60px;
  position:relative;
  text-align:center;
  line-height:120px;
  top:0;
  left:0;
  color:#fff;
  font-size:30px;
  text-decoration:none;
}

ul.circle{
  position:absolute;
  top:77px;
  left:100px;
  list-style:none;
  top:0;
  left:90px;
  li{
    background:#fff;
    border:2px solid #f50;
    width: 100px;
    height:100px;
    line-height:100px;
