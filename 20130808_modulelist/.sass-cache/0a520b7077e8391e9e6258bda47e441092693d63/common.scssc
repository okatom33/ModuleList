.arrow{
  position:absolute;
  top:50%;
  right:30px;
  height:20px;
  line-height:1;
  margin-top:-10px;
  transition:.3s;
  color:#f50;
  text-shadow:none;
  &:before{
    font-size:20px;
    content:">";
    background:#fff;
    border-radius:50%;
    padding:3px 10px;
    color:#f50;
    box-shadow:0 0 3px solid rgba(0,0,0,0.7);
  }
}
.rotate{
  color:#fff;
  -webkit-transform:rotate(90deg);
}


