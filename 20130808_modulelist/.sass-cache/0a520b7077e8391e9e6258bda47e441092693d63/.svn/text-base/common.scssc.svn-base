        border-top:1px solid #888;
        border-bottom:none;
          a{
          position:relative;
          background:none;
          line-height:1;
          padding:15px 70px;
          color:#555;
          text-shadow:none;
          border-top:1px solid rgba(255,100,0,0.3);
          &:before{
            position:absolute;
            top:50%;
            left:45px;
            content:"‚ñ∑";
            font-size:16px;
            color:#f40;
            margin-top:-8px;
            transition:.3s;
          }
        }
        &:first-child a{
          border-top:none;
        }
