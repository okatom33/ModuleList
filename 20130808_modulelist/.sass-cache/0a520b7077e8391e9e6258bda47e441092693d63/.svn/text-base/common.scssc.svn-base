body#modal {
    background-color: #ddd;
    font: 100%/1.5 sans-serif;
    width:320px;
    height:1000px;
}
label {
    @extend %hr;
    cursor: pointer;
    // display: inline-block;
    // padding: .75em 1.5em;
    text-transform: uppercase;
    vertical-align: top;
    // &:hover,
    // &:focus {
    //     background-color: #fffaaa;
    // }
}
input {
    background-color: transparent;
    border-color: transparent;
    height: 0;
    outline: 0;
    position: relative;
    width: 0;
    &:focus + .modal {
        display: inline-block;
        vertical-align: top;
    }
}
.modal {
    background: rgba(0,0,0,.5);
    bottom: 0;
    display: none;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
    width:320px;
    height:100%;
    .modalContent{
      ul.menuList{
        list-style:none;
        width:270px;
        display:block;
        overflow:hidden;
        position:relative;
        li{
          float:left;
          margin-left:6px;
          a{
            @extend %hr;
            width:67px;
            border-radius:2px;
            background:-webkit-gradient(
              linear,left top,left bottom,
              from(#888),
              to(#444)
              );
            border:1px solid #333;
            box-shadow:none;
            &.first{
              width:30px;
              height:260px;
              position:absolute;
              left:0;
              top:0;
            }
            &.last{
              width:30px;
              height:260px;
              position:absolute;
              right:0;
              top:0;
            }
          }
        }
      }
      img {
        position: absolute;
        top: 30px;
        left:5px;
        display:block;
        width:300px;
        height:auto;
      }
      p.close{
        color:#fff;
        position:absolute;
        top: 20px;
        left:5px;
        font-weight:bold;
        font-size:12px;
        left:6px;
        background:rgba(0,0,0,0.4);
        border-radius:6px;
        padding:2px 4px;
      }
    }
  }




  //„Ç´„Éº„Éâ„Çí„ÇÅ„Åè„Çã
  #card {
    border: 1px solid #999;
    width: 320px;
    height: 420px;
    -webkit-perspective: 1000;
    -webkit-transform-style: preserve-3d;
}

#face {
    padding: 5px;
    width: 310px;
    height: 410px;
    background:#222;
}

#front {
    background: #f50;
    position: absolute;
    width: 310px;
    height: 410px;
    -webkit-backface-visibility: hidden;
    text-align:center;
    line-height:310px;
    font-size:30px;
}

#back {
    @extend #front;
    background:#ff9;
    position: absolute;
    -webkit-backface-visibility: hidden;
}

.reversed {
    -webkit-transform: rotateY(180deg);
}

/** Animation: Turn Left **/
.turnleft {
    -webkit-animation-duration: 300ms;
    -webkit-animation-name: turnleft;
}

@-webkit-keyframes turnleft {
    0% { -webkit-transform: rotateY(0deg); }
    100% { -webkit-transform: rotateY(180deg); }
}

/** Animation: Turn Right **/
.turnright {
    -webkit-animation-duration: 300ms;
    -webkit-animation-name: turnright;
}

@-webkit-keyframes turnright {
    0% { -webkit-transform: rotateY(0deg); }
    100% { -webkit-transform: rotateY(-180deg); }
}



// flipcard part2
.cardImage{
    width:320px;
    height:420px;
    position:relative;
    float:left;

    transform-style: preserve-3d;
    -webkit-transform-style: preserve-3d;

    transition: transform 1s;
    -webkit-transition: -webkit-transform 1s;
}
.cardImage figure{
  @extend #front;
    width:100%;
    height:100%;
    margin: 0;
    position: absolute;
    top: 0px;
    left: 0px;
    // backface-visibility: hidden;
    // -webkit-backface-visibility: hidden;

    -webkit-transition: transform 1s;
   &.front{
      background: #f50;
  }
  &.back{
      background:#ff9;

      transform: rotateY(180deg);
      -webkit-transform: rotateY(180deg);
  }
}

.fliped{
    transform: rotateY(180deg);
    -webkit-transform: rotateY( 180deg );
}
